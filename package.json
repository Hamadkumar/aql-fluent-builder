{
  "name": "aql-fluent-builder",
  "version": "1.0.1",
  "description": "Type-safe, fluent AQL query builder for ArangoDB with graph traversal, edge repositories, and OpenAPI spec generation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hamadkumar/aql-fluent-builder.git"
  },
  "homepage": "https://github.com/Hamadkumar/aql-fluent-builder#readme",
  "bugs": {
    "url": "https://github.com/Hamadkumar/aql-fluent-builder/issues"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "dev": "ts-node ./src/aql.test.ts",
    "generate:spec": "ts-node src/scripts/generate-spec.ts",
    "generate:client": "openapi-ts --input openapi.json --output src/client --client @hey-api/client-fetch",
    "build:sdk": "npm run generate:spec && npm run generate:client",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "aql",
    "arangodb",
    "query-builder",
    "typescript",
    "arango",
    "database",
    "nosql",
    "graph-database",
    "query",
    "builder",
    "fluent-api",
    "type-safe",
    "arango-query-language",
    "graph-traversal",
    "edge-collection",
    "openapi",
    "aql-builder",
    "aql-query",
    "orm",
    "odm",
    "typescript-orm",
    "arango-orm",
    "arango-odm",
    "builder-pattern",
    "query-generator",
    "database-client"
  ],
  "author": {
    "name": "Hamad Kumar",
    "email": "hamadkumar.k@gmail.com"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/Hamadkumar"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "~5.9.3"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
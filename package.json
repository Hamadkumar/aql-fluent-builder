{
  "name": "aql-fluent-builder",
  "version": "1.0.0",
  "description": "TypeScript AQL query builder for ArangoDB",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/username/aql-fluent-builder.git"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "dev": "ts-node ./src/aql.test.ts",
    "generate:spec": "ts-node src/scripts/generate-spec.ts",
    "generate:client": "openapi-ts --input openapi.json --output src/client --client @hey-api/client-fetch",
    "build:sdk": "npm run generate:spec && npm run generate:client",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "arangodb",
    "aql",
    "query-builder",
    "typescript",
    "database"
  ],
  "author": "hamadkumar",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "ts-patch": "^3.3.0",
    "typescript": "~5.9.3"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  }
}